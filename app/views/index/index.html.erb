<table width="100%" border="1">
  <thead>
    <th>ID</th>
    <th>Content</th>
    <th>Created at</th>
    <th>Updated at</th>
  </thead>
  <tbody>
  <% if @logs.any? %>
    <% @logs.each do |log| %>
      <tr>
        <td><%= log.id %></td>
        <td><%= log.content %></td>
        <td><%= log.created_at %></td>
        <td><%= log.updated_at %></td>
      </tr>
    <% end %>
  <% end %>
  </tbody>
</table>
